<!--轮播图  -->
<view class="img_swiper">
    <swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{500}}">
        <block ks:for="{{imgUrls}}">
            <swiper-item>
                <image :src="{{item}}" class="slide-image" />
            </swiper-item>
        </block>
    </swiper>
</view>

